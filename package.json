  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .jsx,.js ./",
    "clean": "rm -rf static",
    "build": "yarn run clean && webpack",
    "test": "mocha --compilers js:@babel/register",
    "dev": "better-npm-run dev",
    "staging": "better-npm-run staging",
    "production": "better-npm-run production"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --config webpack.config.dev.babel.js --inline --hot --host 0.0.0.0",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "staging": {
      "command": "yarn run build --config webpack.config.staging.babel.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "staging"
      }
    },
    "production": {
      "command": "yarn run build --config webpack.config.production.babel.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
